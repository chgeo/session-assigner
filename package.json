{
  "name": "session-management",
  "version": "1.0.0",
  "description": "Management for Hands-on Sessions",
  "repository": "https://github.com/chgeo/session-management.git",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^7",
