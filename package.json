  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7",
    "@types/jest": "^29.5.10",
    "axios": "^1.6.2",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0"
  },
  "engines": {
    "node": "^20"
